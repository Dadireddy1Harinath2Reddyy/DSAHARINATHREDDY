// To check the given two strings are not anagram or not 
import java.util.Arrays;
class string1
{ 
   
 public static void main(String[] args) 
	{ 
	 String s1 ="listen";
	 String s2 = "silent";
	 char c1[] = s1.toCharArray();
	 char c2[] = s2.toCharArray();
	 Arrays.sort(c1);
	 Arrays.sort(c2);
	 s1 = new String(c1);
	 s2 = new String(c2);
	 if ( s1.equals(s2))
	 {
		 System.out.println("Yes");
		
	 }


		
    }
}
