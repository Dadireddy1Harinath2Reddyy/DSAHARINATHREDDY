// LEFT  NON MOST REPTION REPTIING CHARACTER 
class string
{ 
	public static void main(String []args)
	{
		String s1 = "geeksforgeeks";
		int char1[] = new int[256];
		for (int i=0;i<s1.length() ;i++ )
		{
			char1[s1.charAt(i)]++;
		}
		for (int i =0;i<s1.length() ;i++ )
		{
			if (char1[s1.charAt(i)]== 1)
			{
				System.out.println("The left most reptiting character in the string : "+i);
				break;
			}

		}
	}
}
